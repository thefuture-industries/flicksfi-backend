<UserControl x:Class="flick_finder.View.Youtube.YoutubeCardHeader"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:flick_finder.View.Youtube"
             mc:Ignorable="d"
             Background="Transparent"
             d:DesignHeight="120" d:DesignWidth="220">
    <Grid>
        <Border Width="200" Height="100"
                CornerRadius="7" Margin="10 0">
            <Border.Background>
                <ImageBrush Stretch="UniformToFill" ImageSource="../../Public/Images/film_test.png"></ImageBrush>
            </Border.Background>
        </Border>
    </Grid>
</UserControl>
